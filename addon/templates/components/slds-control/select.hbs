<div class="slds-select_container">
  <select onchange={{action onSelected value="target.value"}} class="slds-select" id={{formElementId}}>
    <option value="">Please select</option>

    {{#if options}}
      {{#each options as |option|}}
        <option value={{option}} selected={{eq vehicle vehicleChoice}}>{{option}}</option>
      {{/each}}
    {{/if}}
  </select>
</div>