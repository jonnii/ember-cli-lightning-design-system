<span class="slds-checkbox">
  <input type="checkbox" name={{name}} id={{formElementId}} checked={{isChecked}} disabled={{isDisabled}} onclick={{checked}}/>
  <label class="slds-checkbox__label" for={{formElementId}}>
    <span class="slds-checkbox--faux"></span>
    <span class="slds-form-element__label">{{label}}</span>
  </label>
</span>