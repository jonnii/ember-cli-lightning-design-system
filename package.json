{
  "name": "ember-cli-lightning-design-system",
  "version": "0.0.34",
  "description": "Easy installation and reusable components for the salesforce lightning design system.",
  "keywords": [
    "ember-addon",
    "lightning",
    "design system",
    "salesforce"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonnii/ember-cli-lightning-design-system"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "deploy": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push; git checkout -",
    "lint:fix": "eslint --fix ./*.js addon addon-test-support app config lib server test-support tests",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "@salesforce-ux/design-system": "^2.8.3",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.1",
    "ember-cli-babel": "^7.6.0",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-sass": "^10.0.0",
    "resolve": "^1.10.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.9.0-beta.2",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-flash": "^1.7.1",
    "ember-cli-github-pages": "^0.2.1",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-composable-helpers": "^2.2.0",
    "ember-cp-validations": "^4.0.0-beta.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-native-dom-helpers": "^0.6.2",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.1.3",
    "ember-route-action-helper": "^2.0.7",
    "ember-source": "~3.9.0-beta.2",
    "ember-source-channel-url": "1.1.0",
    "ember-truth-helpers": "^2.1.0",
    "ember-try": "1.1.0",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-ember": "^6.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "loader.js": "^4.7.0",
    "prettier": "^1.16.4",
    "qunit-dom": "^0.8.0",
    "sass": "^1.17.3"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://jonnii.github.io/ember-cli-lightning-design-system/",
    "versionCompatibility": {
      "ember": ">=2.8.0"
    }
  }
}
